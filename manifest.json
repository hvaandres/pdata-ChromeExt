{
    "manifest_version": 2,
    "name": "My Job Application Extension",
    "description": "A Chrome extension to autofill job application forms",
    "version": "1.0",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
    }
  }
  